{
  "name": "simple-await",
  "version": "1.0.0",
  "description": "Executing async function with simple error handling. Returns object with data and error.",
  "scripts": {
    "build": "tsc && rollup -c && rimraf compiled",
    "test": "yarn jest"
  },
  "keywords": [
    "async",
    "await",
    "error",
    "handling",
    "ts",
    "promise",
    "try-catch"
  ],
  "author": "aleksfront",
  "license": "MIT",
  "type": "module",
  "main": "dist/simple-await.umd.js",
  "module": "dist/simple-await.es5.js",
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ]
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^10.0.1",
    "@types/jest": "^29.4.0",
    "jest": "^29.3.1",
    "lodash": "^4.17.21",
    "rimraf": "^3.0.2",
    "rollup": "^3.9.1",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-sourcemaps": "^0.6.3",
    "ts-jest": "^29.0.5",
    "typescript": "^4.9.4"
  }
}
